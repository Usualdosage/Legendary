    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tinymce/6.4.0/skins/ui/oxide-dark/skin.shadowdom.min.css" integrity="sha512-miBgTjSXvLM5DC2uQxck2C5CWHiNyagTVVKtEWp8QER2BHf4y4uNDf7o1vgi4BYlpLIjvlgd7VAuJsYgCsaFpw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
