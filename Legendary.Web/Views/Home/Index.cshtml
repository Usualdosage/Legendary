@model UserModel

<div class="console-container">
    <div id="console">
        <p class="loader"><i class="fas fa-spinner fa-spin"></i> One moment, we are loading the game...</p>
    </div>
    <div id="input">
        <input type="text" id="inputField" placeholder="Type message and press enter." />
    </div>
</div>

@section Scripts {
    <script>$(function () {
        const client = new LegacyClient();
        client.Connect();
        $("#inputField").focus();
    });</script>
}